<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template id="snippet_conatact_link" name="Contacts Link">
            <section class="sinppet_contact">
                <div class="kontaktform-block">
                    <div class="container kontaktform">
                        <div class="row">
                            <div class="group-kontaktform">
                                <div class="oe_structure"/>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>
        <template id="vkdata_conatact_link" inherit_id="website.snippets" name="Contacts Link">
            <xpath expr="//div[@id='snippet_structure']/div[2]" position="inside">
                <t t-snippet="theme_vkdata_with_mega_menu.snippet_partners"
                   t-thumbnail="/theme_vkdata_with_mega_menu/static/snippet/snippet_image_001.png"/>
            </xpath>
        </template>
    </data>
</odoo>