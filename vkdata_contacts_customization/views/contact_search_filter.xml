<odoo>
<data>

    <record id="vkdata_forms_view_res_partner_filter" model="ir.ui.view">
        <field name="name">vkdata.forms_view_res_partner_filter</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="after">
                <field name="phone"/>
                <field name="vat"/>
                <field name="city"/>
            </xpath>
        </field>
    </record>
</data>
</odoo>